extends layout

block page-specific
	- var show_h1 = 1
	- var page_title = 'О компании'

	.content-page
		ul.cars.left
			each i in ['1','2','3','4','5','6']
				li
					img(alt="", src=static_url('tmp/left/' + i + '.jpg'))
		.content
			if show_h1
				if page_title
					h1=page_title
			main
				include tmp_text
		ul.cars.right
			each i in ['1','2','3','4','5','6']
				li
					img(alt="", src=static_url('tmp/right/' + i + '.jpg'))
